 => {\n          console.log(e);\n        });\n    },\n    findList() {\n      http\n        .get(\"/sportEquipment/\" + this.sportEquipment.Listid)\n        .then(response => {\n          console.log(response);\n          if (response.data != null) {\n            this.Listid = response.data.sportequipmentid;\n            this.ListCheck = response.status;\n          } else {\n            this.clear()\n          }          \n        })\n        .catch(e => {\n          console.log(e);\n        });\n      this.submitted = true;\n    },\n    deleteList() {\n      http\n        .delete(\"/sportEquipment/\" + this.sportEquipment.Listid)\n        .then(response => {\n          console.log(response.data);\n          this.$emit(\"refreshData\");\n          alert(\"ทำการลบรายการข้อมูลของอุปกรณ์เรียบร้อย\");\n          location.reload();\n        })\n        .catch(e => {\n          console.log(e);\n        });\n    },\n    pushReserve(){\n      this.$router.push(\"/reservation\");\n    },\n  pushBorrow(){\n      this.$router.push(\"/borrow\");\n    },\n  pushCustomer(){\n      this.$router.push(\"/customer\");\n    },\n  pushEmployee(){\n      this.$router.push(\"/employee\");\n    },\n  pushPayment(){\n      this.$router.push(\"/paymentmember\");\n    },\n    pushSportEquipment(){\n      this.$router.push(\"/sportequipment\");\n    },\n    refreshList() {\n      this.getList();\n    }\n    /* eslint-disable no-console */\n  },\n  mounted() {\n    this.getList();\n  }\n};\n</script>\n"]}]}